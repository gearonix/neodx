{
  "name": "example-svg",
  "version": "0.0.1",
  "private": true,
  "packageManager": "yarn@3.2.0",
  "type": "module",
  "sideEffects": false,
  "publishConfig": {
    "access": "restricted"
  },
  "dependencies": {
    "@fontsource-variable/inter": "^5.0.5",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@neodx/svg": "workspace:*",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "10.4.14",
    "postcss": "8.4.26",
    "tailwindcss": "3.3.3",
    "typescript": "^5.1.6",
    "vite": "^4.4.5",
    "vite-tsconfig-paths": "^4.2.0"
  },
  "scripts": {
    "dev": "vite",
    "lint": "eslint src",
    "nuke": "rm -rf dist",
    "build": "vite build",
    "start": "vite preview",
    "typecheck": "tsc --noEmit"
  },
  "nx": {
    "targets": {
      "build": {
        "outputs": [
          "public/sprites"
        ],
        "inputs": [
          "{projectRoot}/assets/**/*.svg",
          "shared",
          "config"
        ],
        "dependsOn": [
          "^build"
        ]
      },
      "test": {
        "dependsOn": [
          "build"
        ]
      }
    }
  }
}
